  "scripts": {
    "dev": "astro dev",
    "build": "npm run licenses && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "licenses": "license-checker --production --excludePrivatePackages --onlyAllow \"MIT;ISC;Apache-2.0;BSD-2-Clause;BSD-3-Clause;LGPL-3.0-or-later;BSD*;Python-2.0;CC0-1.0;(Apache-2.0 AND MIT);0BSD;(MIT OR CC0-1.0);MIT*\" --json --out licenses.json",
    "fmt": "prettier . --write",
    "lnt": "prettier --check . && eslint \"src/**/*.{js,ts,astro}\"",
    "lnt:fix": "prettier --check . && eslint \"src/**/*.{js,ts,astro}\" --fix",
    "check": "astro sync && astro check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "@astrojs/svelte": "^7.1.0",
    "@threlte/core": "^8.0.4",
    "@threlte/extras": "^9.2.1",
