    "@threlte/core": "^8.3.1",
    "@threlte/extras": "^9.7.1",
    "astro": "^5.16.11",
    "astro-embed": "^0.12.0",
    "embla-carousel": "^8.6.0",
    "embla-carousel-auto-scroll": "^8.6.0",
    "sass": "^1.97.2",
    "sharp": "^0.34.5",
