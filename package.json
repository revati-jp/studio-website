    "prettier": "3.8.0",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-svelte": "^3.4.1",
    "svelte-preprocess": "^6.0.3",
    "typescript-eslint": "^8.53.0"
  },
  "packageManager": "pnpm@10.28.1",
  "devEngines": {
    "packageManager": {
      "name": "pnpm",
      "version": "10.28.1",
      "onFail": "error"
    }
