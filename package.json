    "check": "astro sync && astro check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "@astrojs/svelte": "^7.2.5",
    "@fontsource/montserrat": "^5.2.8",
    "@fontsource/noto-sans-jp": "^5.2.9",
